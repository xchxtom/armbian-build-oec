+static ssize_t wtd_write(struct file *file, const char *buffer,size_t count, loff_t *data)
+{
+//	int * add;
+
+	panic_test = 1;
+	return count;
+/*
+	add = ioremap(0x0, 0x100);
+	*add = 0;
+			
+	return count;
+*/
+}
+
+#if LINUX_VERSION_CODE >= KERNEL_VERSION(5,6,0)
+static const struct proc_ops wtd = {
